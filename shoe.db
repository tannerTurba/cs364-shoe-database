CREATE TABLE Customer (
    CustomerId INT PRIMARY KEY AUTOINCREMENT,
    PhoneNumber INT NOT NULL,
    Email NVARCHAR(25) NOT NULL, 
    StreetAddress NVARCHAR(15) NOT NULL, --composite attribute?(address)
    CityAddress NVARCHAR(10) NOT NULL, 
    StateAddress NVARCHAR(10) NOT NULL, 
    ZipAddress INT NOT NULL, 
    FirstName NVARCHAR(10) NOT NULL, --composite attribute?(name)
    LastName NVARCHAR(10) NOT NULL
);

CREATE TABLE Model (
    ModelId NVARCHAR(30) PRIMARY KEY NOT NULL,
    StyleId NVARCHAR(10) NOT NULL,
    Price NUMERIC(8,2) NOT NULL, 
    ShoeType NVARCHAR(10) NOT NULL,
    ModelName NVARCHAR(15) NOT NULL, 
    Color NVARCHAR(10) NOT NULL,
    Thumbnail NVARCHAR(190)
);

CREATE TABLE Brand (
    BrandId INT PRIMARY KEY AUTOINCREMENT,
    YearEstablished INT NOT NULL, 
    AddressStreet NVARCHAR(15) NOT NULL, --composite attribute?(address) Yes
    AddressCity NVARCHAR(10) NOT NULL, 
    AddressState NVARCHAR(10) NOT NULL, 
    AddressZip INT NOT NULL, 
    Name NVARCHAR(10) NOT NULL
);

CREATE TABLE Buys (
    PaymentType NVARCHAR(10) NOT NULL, --set valued? No, only one payment type
    ReceiptNumber INT NOT NULL, 
    TotalCost FLOAT NOT NULL, 
    Quantity INT NOT NULL, 
    Gender NVARCHAR(10) NOT NULL, 
    Size NUMERIC(8,2) NOT NULL, 
    Color NVARHCAR(10) NOT NULL
    CONSTRAINT fk_customer_id FOREIGN KEY (CustomerId) REFERENCES Customer(CustomerId), 
    CONSTRAINT fk_model_id FOREIGN KEY (ModelId) REFERENCES Model(ModelId)
);

CREATE TABLE Makes (
    ReleaseDate NVARCHAR(10) NOT NULL, 
    CONSTRAINT fk_brand_id FOREIGN KEY (Brand) REFERENCES Brand(BrandId),
    CONSTRAINT fk_model_id FOREIGN KEY (Model) REFERENCES Model(ModelId)
);